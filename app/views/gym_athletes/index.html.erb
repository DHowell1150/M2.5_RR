<h1> <%= @gym.name %>'s Athletes  </h1>

<h3><% @athletes.each do |athlete| %> </h3>
  <div id="athlete-<%= athlete.id %>">
    <h3> <%= athlete.name %> </h3>
    <p>Age: <%= athlete.age %> </p>
    <p> <%= athlete.collegiate_athlete? %> </p>
  </div>
<% end %>
<%= link_to "Add Athlete", "/gyms/#{@gym.id}/gym_athletes/new" %>
