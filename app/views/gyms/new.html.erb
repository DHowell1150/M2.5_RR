<h1> New Gym  </h1>
<%= form_with url: "/gyms", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :num_bikes %>
  <%= f.text_field :num_bikes %>

  <%= f.label :outdoor_space %>
  <%= f.text_field :outdoor_space %>

  <%= f.submit "Create Gym"%>
<% end %>